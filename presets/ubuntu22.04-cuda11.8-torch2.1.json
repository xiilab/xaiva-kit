  },
  "tensorrt": {
    "version": "8.6.1"
  },
  "cuda": {
    "arch": "86"
  },
  "build_options": {
    "ffmpeg_version": "4.2",
    "opencv_version": "4.11.0",
    "xaiva_media_source": {
      "path": "xaiva-media",
      "branch": "feature/standardize-cuda11-pytorch21"
    }
